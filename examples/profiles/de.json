{
    "optimization": {
        "type": "ntc",
        "solver": "gurobi",
        "model_horizon": [0, 24],
        "heat_model": false,
        "redispatch": {
                "include": true,
                "cost": 1
            },
        "infeasibility": {
            "heat": {
                "include": false,
                "cost": 1E3,
                "bound": 1000
            },
            "electricity": {
                "include": true,
                "cost": 2000,
                "bound": 1000
            },
            "lines": {
                "include": false,
                "cost": 1E3,
                "bound": 1000
            }
        },
        "curtailment": {
            "include": true,
            "cost": 100
        },
        "parameters": {
            "storage_start": 0.65
        },
        "plant_types": {
            "es": ["hydro_res", "hydro_psp"],
            "hs": [],
            "ts": ["wind", "solar"],
            "ph": []
        }
    },
    "grid": {
        "cbco_option": "clarkson_base",
        "precalc_filename": "",
        "senstitivity": 5e-2,
        "capacity_multiplier": 0.95,
        "preprocess": true,
        "gsk": "gmax"
    },
    "data": {
        "data_type": "opsd",
        "stacked": ["demand_el"],
        "process": ["net_export", "availability", "net_position", "opsd"],
        "process_input": true,
        "unique_mc": false,
        "round_demand": true,
        "default_efficiency": 0.5,
        "default_mc": 200,
        "co2_price": 20
    }
}